{
  "name": "lazy-event-router",
  "version": "6.0.3",
  "description": "an event router",
  "keywords": [],
  "main": "./dist/lib/lazy-event-router.js",
  "types": "./dist/lib/lazy-event-router.d.ts",
  "files": [
    "dist",
    "web"
  ],
  "scripts": {
    "dist": "npm run node && npm run web",
    "node": "rm -rf dist && cross-env NODE_ENV=production webpack",
    "web": "rm -rf web && cross-env NODE_ENV=production webpack --config ./webpack.config.web.js",
    "lint": "npm run eslint && npm run tslint",
    "eslint": "eslint *.js src test",
    "prepublish": "npm run dist",
    "tslint": "tslint  tslint *.ts src/**/*.ts test/**/*.ts",
    "test": "npm run test-node && npm run test-web",
    "test-node": "cross-env NODE_ENV=test nyc mocha",
    "test-web": "cross-env NODE_ENV=test CLI=1 karma start --single-run",
    "test-web-gui": "cross-env NODE_ENV=test karma start --single-run",
    "docdist": "cd doc && git clean -fd && git rm -rf * && cd .. && npm run doc && cd doc && touch .nojekyll && git add . && git commit -m doc && git push",
    "doc": "typedoc --mode modules --out doc --readme Readme.md --name \"API Document\" --disableOutputCheck --includeDeclarations --excludeNotExported src && cd doc && git status && cd .."
  },
  "dependencies": {
    "babel-runtime": "^6.23.0"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.41",
    "@types/node": "^7.0.27",
    "@types/power-assert": "^1.4.29",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-power-assert": "^1.0.0",
    "babel-register": "^6.24.1",
    "browserfs": "^1.3.0",
    "cross-env": "^5.0.0",
    "eslint": "^3.19.0",
    "eslint-config-narazaka": "^1.0.1",
    "eslint-loader": "^1.7.1",
    "espower-ts-node": "^1.0.3",
    "istanbul-instrumenter-loader": "^0.2.0",
    "json-loader": "^0.5.4",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.1.1",
    "karma-config-narazaka-ts-js": "^1.2.0",
    "karma-coverage": "^1.1.1",
    "karma-detect-browsers": "^2.2.5",
    "karma-edge-launcher": "^0.4.1",
    "karma-firefox-launcher": "^1.0.1",
    "karma-ie-launcher": "^1.0.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-own-reporter": "^1.1.2",
    "karma-opera-launcher": "^1.0.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-safari-launcher": "^1.0.0",
    "karma-webpack": "^2.0.3",
    "mocha": "^3.4.2",
    "nyc": "^11.0.2",
    "phantomjs-prebuilt": "^2.1.14",
    "power-assert": "^1.4.3",
    "ts-loader": "^2.1.0",
    "ts-node": "^3.0.4",
    "tslint": "^5.4.2",
    "tslint-loader": "^3.5.3",
    "typedoc": "^0.7.1",
    "typescript": "^2.3.4",
    "webpack": "^1.13.3",
    "webpack-config-narazaka-ts-js": "0.2.0"
  },
  "url": "https://github.com/Narazaka/lazy-event-router.js",
  "author": "Narazaka",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Narazaka/lazy-event-router.js.git"
  }
}
